{
  "ignorePatterns": [
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "aka\\.ms"
    },
    {
      "pattern": "^https?://"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "./"
    }
  ],
  "aliveStatusCodes": [200, 206],
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "5s",
  "timeout": "10s"
}
